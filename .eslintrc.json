{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "plugins": ["prettier"],
    "extends": [
        "airbnb-base"
      ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "prettier/prettier": "error",
      "no-eval": ["error", {"allowIndirect": true}],
      "no-alert": "off",
      "no-return-assign": "off"
    }
}
